{
  "name": "@deptdk/liquidjs-renderer",
  "version": "0.2.2",
  "description": "Storybook LiquidJS renderer",
  "homepage": "https://github.com/sorthvidk/storybook-liquidjs-framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/sorthvidk/storybook-liquidjs-framework.git",
    "directory": "packages/liquidjs-renderer"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preview": {
      "require": "./dist/config.js",
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.ts"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json",
      "types": "./package.json"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "build": "../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@storybook/addons": "^7.0.0",
    "@storybook/core-client": "^7.0.0",
    "@storybook/csf": "~0.1.0",
    "@storybook/docs-tools": "^7.0.0",
    "@storybook/preview-web": "^7.0.0",
    "@storybook/store": "^7.0.0",
    "global": "^4.4.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "ts-dedent": "^2.0.0"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.18.0",
    "liquidjs": "^10.7.1",
    "typescript": "~4.6.3"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "liquidjs": "^10.7.1"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/config.ts"
    ],
    "platform": "browser"
  }
}
